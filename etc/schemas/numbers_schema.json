{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Number set",
    "type": "array",
    "items": {
        "title": "Number",
        "type": "object",
        "properties": {
            "apiVersion": {
              "enum": ["v1beta1", "v1.0", "v1"]
            },
            "kind": {
                "enum": ["Number", "Number"],
                "readOnly": true
            },
            "metadata": {
                "type": "object",
                "properties": {
                    "createdOn": {"type": "string", "readOnly": true},
                    "modifiedOn": {"type": "string", "readOnly": true},
                    "ref": {"type": "string", "readOnly": true, "minLength": 3, "maxLength": 24},
                    "gwRef": {"type": "string", "readOnly": true, "minLength": 3, "maxLength": 24}
                },
                "required": ["gwRef"]
            },
            "spec": {
                "type": "object",
                "properties": {
                    "location": {
                        "type": "object",
                        "properties": {
                            "telUrl": {"type": "string", "minLength": 4},
                            "aorLink": {"type": "string", "minLength": 4}
                        },
                        "required": ["telUrl", "aorLink"]
                    }
                },
                "required": ["location"]
            }
        },
        "required": ["apiVersion", "kind", "metadata", "spec", "metadata"]
    }
}
