{
  "name": "routr",
  "version": "1.2.1",
  "description": "Next-generation SIP Server",
  "scripts": {
    "prepack": "npm test",
    "test": "cross-env ./etc/tests/tester2 mod/**/*.unit.test.js",
    "test-kt": "gradle test --tests *Test",
    "pretest": "npm run bundle",
    "bundle": "webpack --config webpack.config.js --mode none",
    "prebundle": "cross-env ./gradlew build copyRuntimeLibs && npm run transpile && cp node_modules/jvm-npm/src/main/javascript/jvm-npm.js libs",
    "transpile": "tsc",
    "format": "prettier-standard 'mod/**/*.js'",
    "clean": "rimraf node_modules build libs .gradle .routr.salt *.dat",
    "create:helm-chart": "helm package .helm/ -d .charts && helm repo index .charts",
    "create:jre": "./etc/create-jre.sh",
    "distro:windows": "./distro.sh windows jre-11.0.10_windows-x64_bin.tar.gz",
    "distro:darwin": "./distro.sh osx jre-11.0.9.1_osx-x64_bin.tar.gz",
    "distro:linux": "./distro.sh linux jre-11.0.15+10_linux-x64_bin.tar.gz"
  },
  "lint-staged": {
    "mod/**/*.js": [
      "prettier-standard"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "dependencies": {
    "bencode": "^2.0.1",
    "buffer": "^6.0.3",
    "deepmerge": "^4.2.2",
    "flat": "^5.0.0",
    "ip-utils": "^2.4.0",
    "moment": "^2.22.2",
    "paginate-array": "^2.1.0",
    "phone": "^2.4.20",
    "postal": "^2.0.5",
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "@types/node": "^13.13.2",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "husky": "^4.2.3",
    "jvm-npm": "^0.1.1",
    "lint-staged": "^10.0.9",
    "mocha": "^6.2.3",
    "npm": "^8.6.0",
    "nyc": "^14.1.1",
    "prettier-standard": "^9.1.1",
    "rewire": "^5.0.0",
    "rimraf": "^2.7.1",
    "sinon": "^9.0.1",
    "sinon-chai": "^3.5.0",
    "source-map-loader": "^0.2.4",
    "typescript": "^4.6.3",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/fonoster/routr.git"
  },
  "author": "Pedro Sanders",
  "license": "MIT"
}
